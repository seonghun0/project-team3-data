"  return ( (v/(v+m)) * R ) + ( (m/(m+v)) * C )  # 가중 평점 계산 식\n","\n","movies1_df['weighted_vote'] = movies.apply(weighted_vote_average, axis=1)\n","\n","movies1_df[['title', 'weighted_vote', 'vote_count']].sort_values('weighted_vote', ascending=False)[:10]"],"execution_count":null,"outputs":[{"output_type":"execute_result","data":{"text/html":["<div>\n","<style scoped>\n","    .dataframe tbody tr th:only-of-type {\n","        vertical-align: middle;\n","    }\n","\n","    .dataframe tbody tr th {\n","        vertical-align: top;\n","    }\n","\n","    .dataframe thead th {\n","        text-align: right;\n","    }\n","</style>\n","<table border=\"1\" class=\"dataframe\">\n","  <thead>\n","    <tr style=\"text-align: right;\">\n","      <th></th>\n","      <th>title</th>\n","      <th>weighted_vote</th>\n","      <th>vote_count</th>\n","    </tr>\n","  </thead>\n","  <tbody>\n","    <tr>\n","      <th>195</th>\n","      <td>대부</td>\n","      <td>8.274009</td>\n","      <td>15086.0</td>\n","    </tr>\n","    <tr>\n","      <th>118</th>\n","      <td>다크 나이트</td>\n","      <td>8.257540</td>\n","      <td>26213.0</td>\n","    </tr>\n","    <tr>\n","      <th>88</th>\n","      <td>반지의 제왕: 왕의 귀환</td>\n","      <td>8.176728</td>\n","      <td>18584.0</td>\n","    </tr>\n","    <tr>\n","      <th>86</th>\n","      <td>반지의 제왕: 반지 원정대</td>\n","      <td>8.113440</td>\n","      <td>20014.0</td>\n","    </tr>\n","    <tr>\n","      <th>87</th>\n","      <td>반지의 제왕: 두 개의 탑</td>\n","      <td>8.077368</td>\n","      <td>17295.0</td>\n","    </tr>\n","    <tr>\n","      <th>130318</th>\n","      <td>어벤져스: 인피니티 워</td>\n","      <td>8.061757</td>\n","      <td>23139.0</td>\n","    </tr>\n","    <tr>\n","      <th>130316</th>\n","      <td>어벤져스: 엔드게임</td>\n","      <td>8.026017</td>\n","      <td>19559.0</td>\n","    </tr>\n","    <tr>\n","      <th>1296</th>\n","      <td>스타워즈: 에피소드 5 제국의 역습</td>\n","      <td>8.009502</td>\n","      <td>13541.0</td>\n","    </tr>\n","    <tr>\n","  